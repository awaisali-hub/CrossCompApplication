<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appbar_layout"
        android:layout_width="match_parent"
        android:layout_height="72dp">
        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/blue">

            <TextView
                android:id="@+id/userNAME"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="20sp"
                android:text="Affiliate Dashboard"
                android:layout_marginEnd="15dp"
                android:gravity="center"
                android:textColor="@color/actionbar_text_colour"
                android:textStyle="bold"/>
        </androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.AppBarLayout>



    <LinearLayout
        android:layout_below="@+id/appbar_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/_10sdp">

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team1"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:textAllCaps="false"
            android:layout_below="@+id/worldTeam"
            android:background="@drawable/res"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Judge"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:onClick="Judge"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team5"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:layout_below="@+id/worldTeam"
            android:background="@drawable/res"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textAllCaps="false"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Team Captain"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:onClick="MoveToTeamCaptain"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team2"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:layout_below="@+id/team1"
            android:background="@drawable/res"
            android:textAllCaps="false"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Service Coordinator"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:onClick="ServiceCoordinator"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team3"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:layout_below="@+id/team2"
            android:background="@drawable/res"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textAllCaps="false"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Team Manager"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:onClick="MoveToTeamManager"
            />



        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team6"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:layout_below="@+id/team1"
            android:background="@drawable/res"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textAllCaps="false"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Store"
            android:textColor="@color/white"
            android:textSize="18sp"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/team7"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:layout_below="@+id/team2"
            android:background="@drawable/res"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:textAllCaps="false"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="30dp"
            android:layout_marginBottom="15dp"
            android:text="Earnings"
            android:textColor="@color/white"
            android:textSize="18sp"
            />

    </LinearLayout>



    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fab"
        android:layout_width="@dimen/_43sdp"
        android:layout_height="@dimen/_43sdp"
        android:src="@drawable/crosscompicon2"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="24dp"
        android:onClick="HomeScore"/>


</RelativeLayout>